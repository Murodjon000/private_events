<h1>Log in</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_with(url: sessions_path, local: true) do |f|%>
           
            <%= f.label :name%>
            <%= f.text_field :name, class: "form-control" %>

            <%= f.submit "Log in",class:"btn btn-primary" %>
        <% end %>
    </div>
</div>